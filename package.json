{
  "name": "phpcgiserver",
  "version": "1.0.0",
  "type": "module",
  "description": "PHP FastCGI Server on Node.JS",
  "author": {
    "name": "Alex M."
  },
  "keywords": [
    "php",
    "cgi",
    "fastcgi",
    "server",
    "node",
    "node.js",
    "https"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BioFieldUA/phpcgiserver.git"
  },
  "license": "MIT",
  "main": "index.js",
  "exports": {
    ".": [ "./index.js", "./js/utils.js", "./js/fastcgi.js" ]
  },
  "files": [
    "index.js",
    "server.js",
    "js/*.js",
    "*.pem",
    "views/*.ejs",
    "public/public_html/index.php"
  ],
  "scripts": {
    "start": "node server"
  },
  "dependencies": {
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "fastcgi-client": "^0.2.0",
    "https": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "eslint": "^9.16.0"
  }
}
